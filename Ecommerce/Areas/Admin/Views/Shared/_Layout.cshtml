
<!DOCTYPE html>
<html lang="ar">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>مسؤول الأرجواني</title>
    <!-- plugins:css -->
    <link rel="stylesheet" href="~/assets/vendors/mdi/css/materialdesignicons.min.css">
    <link rel="stylesheet" href="~/assets/vendors/css/vendor.bundle.base.css">
    <!-- endinject -->
    <!-- Plugin css for this page -->
    <link rel="stylesheet" href="~/assets/vendors/font-awesome/css/font-awesome.min.css" />
    <link rel="stylesheet" href="~/assets/vendors/bootstrap-datepicker/bootstrap-datepicker.min.css">
    <!-- End plugin css for this page -->
    <!-- inject:css -->
    <!-- endinject -->
    <!-- Layout styles -->
    <link rel="stylesheet" href="~/assets/css/demo_1/style.css">
    <!-- End layout styles -->
    <link rel="shortcut icon" href="~/assets/images/favicon.png" />
    <link rel="stylesheet" href="path/to/font-awesome/css/all.min.css">
  </head>
  <body class="rtl">
    <div class="container-scroller">
      <!-- navbar starts here -->
      <nav class="navbar default-layout-navbar col-lg-12 col-12 p-0 fixed-top d-flex flex-row">

          <!-- Logo starts here -->
        <div class="text-center navbar-brand-wrapper d-flex align-items-center justify-content-center">
          <a class="navbar-brand brand-logo" href="../../index.html"><img src="~/assets/images/logo.svg" alt="logo" /></a>
          <a class="navbar-brand brand-logo-mini" href="../../index.html"><img src="~/assets/images/logo-mini.svg" alt="logo" /></a>
        </div>
          <!-- Logo ends here -->

        <div class="navbar-menu-wrapper d-flex align-items-stretch">

           <!-- navbar menu button starts here -->
          <button class="navbar-toggler navbar-toggler align-self-center" type="button" data-toggle="minimize">
            <span class="mdi mdi-menu"></span>
          </button>
           <!-- navbar menu button ends here -->

           <!-- search starts here -->
          <div class="search-field d-none d-md-block">
            <form class="d-flex align-items-center h-100" action="#">
              <div class="input-group">
                <div class="input-group-prepend bg-transparent">
                  <i class="input-group-text border-0 mdi mdi-magnify"></i>
                </div>
                <input type="text" class="form-control bg-transparent border-0" placeholder=" Search">
              </div>
            </form>
          </div>
           <!-- search ends here -->

          <!-- profile starts here -->
          <ul class="navbar-nav navbar-nav-right pe-0">
            <li class="nav-item nav-profile dropdown">

                <!-- Imge and userName profile starts here -->
              <a class="nav-link dropdown-toggle" id="profileDropdown" href="#" data-bs-toggle="dropdown" aria-expanded="false">
                <div class="nav-profile-img">
                  <img src="~/assets/images/faces/face1.jpg" alt="image">
                  <span class="availability-status online"></span>
                </div>
                <div class="nav-profile-text">
                  <p class="mb-1 text-black">UserName</p>
                </div>
              </a>
                <!-- Imge and userName profile ensds here -->

              <!--dropdown menu profile starts here -->
              <div class="dropdown-menu navbar-dropdown" aria-labelledby="profileDropdown">
                <a class="dropdown-item" href="#">
                  <i class="mdi mdi-cached me-2 text-success"></i> سجل النشاطات </a>
                <div class="dropdown-divider"></div>
                <a class="dropdown-item" href="#">
                  <i class="mdi mdi-logout me-2 text-primary"></i> Logout </a>
              </div>
              <!--dropdown menu profile ends here -->

            </li>
            <!-- full screen profile starts here -->
            <li class="nav-item d-none d-lg-block full-screen-link">
              <a class="nav-link">
                <i class="mdi mdi-fullscreen" id="fullscreen-button"></i>
              </a>
            </li>
            <!--  full screen profile ends here -->

          </ul>
          <!-- profile ends here -->

        </div>
      </nav>

      <!-- navbar ends here -->

      <div class="container-fluid page-body-wrapper">

        <!-- sidebar starts here -->
        <nav class="sidebar sidebar-offcanvas" id="sidebar">
          <ul class="nav">
            <li class="nav-item nav-profile">

             <!-- profile Image, UserName and JobTitle  starts here -->
              <a href="#" class="nav-link">
                <div class="nav-profile-image">
                  <img src="~/assets/images/faces/face1.jpg" alt="profile">
                  <span class="login-status online"></span>
                  <!--change to offline or busy as needed-->
                </div>
                <div class="nav-profile-text d-flex flex-column">
                  <span class="font-weight-bold mb-2">UserName</span>
                  <span class="text-secondary text-small">JobTitle</span>
                </div>
                <i class="mdi mdi-bookmark-check text-success nav-profile-badge"></i>
              </a>
             <!-- profile Image, UserName and JobTitle  ends here -->

            </li>
            <!-- profile MainPage starts here -->
            <li class="nav-item">
              <a class="nav-link" asp-area="Admin" asp-controller="Home" asp-action="Index">
                <span class="menu-title">MainPage</span>
                <i class="mdi mdi-home menu-icon"></i>
              </a>
            </li>
            <!-- profile MainPage  ends here -->

            <!-- profile Product starts here -->
            <li class="nav-item">
              <a class="nav-link" data-bs-toggle="collapse" href="#Products" aria-expanded="false" aria-controls="Products">
                <span class="menu-title">@Resources.Products</span>
                <i class="menu-arrow"></i>
                <i class="mdi mdi-package-variant menu-icon"></i>
              </a>
              <div class="collapse" id="Products">
                <ul class="nav flex-column sub-menu">
                  <li class="nav-item"> <a class="nav-link" asp-area="Admin" asp-controller="Tool" asp-action="Index">@Resources.ProductList</a></li>
                  <li class="nav-item"> <a class="nav-link" asp-area="Admin" asp-controller="Tool" asp-action="Create">@Resources.AddProduct</a></li>
                </ul>
              </div>
            </li>

            <!-- profile Product ends here -->

            <!-- profile Departments starts here -->
            <li class="nav-item">
              <a class="nav-link" data-bs-toggle="collapse" href="#Departments" aria-expanded="false" aria-controls="Departments">
                <span class="menu-title">@Resources.Departments</span>
                <i class="menu-arrow"></i>
                <i class="mdi mdi-package-variant menu-icon"></i>
              </a>
              <div class="collapse" id="Departments">
                <ul class="nav flex-column sub-menu">
                  <li class="nav-item"> <a class="nav-link" asp-area="Admin" asp-controller="Department" asp-action="Index">@Resources.DepartmentList</a></li>
                  <li class="nav-item"> <a class="nav-link" asp-area="Admin" asp-controller="Department" asp-action="Create">@Resources.Addepartment</a></li>
                </ul>
              </div>
            </li>
            <!-- profile Departments ends here -->

            <!-- profile Categorys starts here -->
            <li class="nav-item">
              <a class="nav-link" data-bs-toggle="collapse" href="#Categorys" aria-expanded="false" aria-controls="Categorys">
                <span class="menu-title">@Resources.Categorys</span>
                <i class="menu-arrow"></i>
                <i class="mdi mdi-package-variant menu-icon"></i>
              </a>
              <div class="collapse" id="Categorys">
                <ul class="nav flex-column sub-menu">
                  <li class="nav-item"> <a class="nav-link" asp-area="Admin" asp-controller="Category" asp-action="Index">@Resources.CategoryList</a></li>
                  <li class="nav-item"> <a class="nav-link" asp-area="Admin" asp-controller="Category" asp-action="Create">@Resources.AddCategory</a></li>
                </ul>
              </div>
            </li>
            <!-- profile Categorys ends here -->

            <!-- profile Customers starts here -->
            <li class="nav-item">
              <a class="nav-link" data-bs-toggle="collapse" href="#Customers" aria-expanded="false" aria-controls="Customers">
                <span class="menu-title">Customers</span>
                <i class="menu-arrow"></i>
                <i class="mdi mdi-account-multiple menu-icon"></i>


              </a>
              <div class="collapse" id="Customers">
                <ul class="nav flex-column sub-menu">
                  <li class="nav-item">
                      <a class="nav-link" href="../../pages/apps/kanban-board.html">AllCustomers</a>
                   </li>
                   <li class="nav-item">
                      <a class="nav-link" data-bs-toggle="collapse" href="#CustomersDetails" aria-expanded="false" aria-controls="CustomersDetails">
                        <span class="menu-title">Customers Details</span>
                        <i class="menu-arrow"></i>
                      </a>
                      <div class="collapse" id="CustomersDetails">
                        <ul class="nav flex-column sub-menu">
                          <li class="nav-item"><a class="nav-link" href="../../pages/apps/customer-details.html">Customers Details</a></li>
                          <li class="nav-item"><a class="nav-link" href="../../pages/apps/customer-orders.html">Customer Orders</a></li>
                          <li class="nav-item"><a class="nav-link" href="../../pages/apps/customer-invoices.html">Customer Invoices</a></li>
                        </ul>
                      </div>
                    </li>
                  

                </ul>
              </div>
            </li>
            <!-- profile Customers ends here -->

            <!-- profile Orders starts here -->
            <li class="nav-item">
              <a class="nav-link" data-bs-toggle="collapse" href="#apps" aria-expanded="false" aria-controls="apps">
                <span class="menu-title">Orders</span>
                <i class="menu-arrow"></i>
                <i class="mdi mdi-cart-arrow-down menu-icon"></i>
              </a>
              <div class="collapse" id="apps">
                <ul class="nav flex-column sub-menu">
                  <li class="nav-item"> <a class="nav-link" href="../../pages/apps/kanban-board.html">OrderList</a></li>
                  <li class="nav-item"> <a class="nav-link" href="../../pages/apps/todo.html">OrderDetails</a></li>
                </ul>
              </div>
            </li>
            <!-- profile Orders ends here -->

            <!-- profile ecommerce starts here -->
            <li class="nav-item">
              <a class="nav-link" data-bs-toggle="collapse" href="#e-commerce" aria-expanded="false" aria-controls="e-commerce">
                <span class="menu-title">ecommerce</span>
                <i class="menu-arrow"></i>
                <i class="mdi mdi-shopping menu-icon"></i>
              </a>
              <div class="collapse" id="e-commerce">
                <ul class="nav flex-column sub-menu">
                  <li class="nav-item"> <a class="nav-link" href="../../pages/samples/invoice.html"> فاتورة </a></li>
                  <li class="nav-item"> <a class="nav-link" href="../../pages/samples/pricing-table.html"> جدول التسعير </a></li>
                  <li class="nav-item"> <a class="nav-link" href="../../pages/samples/product-catalogue.html"> كتالوج المنتج </a></li>
                </ul>
              </div>
            </li>
            <!-- profile ecommerce ends here -->

          </ul>
        </nav>
        <!-- sidebar ends here -->

        <!-- main panel starts here -->
        <div class="main-panel">
          <main role="main" class="pb-3">
                    @*<partial name="_Notification" />*@
                    @RenderBody()
          </main>
          <!-- content-wrapper ends -->
          <!-- footer starts here-->
          <footer class="footer">
            <div class="d-sm-flex justify-content-center justify-content-sm-between">
              <span class="text-muted text-center text-sm-left d-block d-sm-inline-block">حقوق الطبع والنشر © 2017 <a href="https://www.bootstrapdash.com/" target="_blank">BootstrapDash</a>. كل الحقوق محفوظة.</span>
              <span class="float-none float-sm-right d-block mt-1 mt-sm-0 text-center">مصنوعة يدويا ومصنوعة من <i class="mdi mdi-heart text-danger"></i></span>
            </div>
          </footer>
          <!-- ends here -->
        </div>
        <!-- main panel ends -->

      </div>
      <!-- page-body-wrapper ends -->
    </div>
    <!-- container-scroller -->
    <!-- plugins:js -->
    <script src="~/assets/vendors/js/vendor.bundle.base.js"></script>
    <!-- endinject -->
    <!-- Plugin js for this page -->
    <script src="~/assets/vendors/chart.js/Chart.min.js"></script>
    <script src="~/assets/vendors/bootstrap-datepicker/bootstrap-datepicker.min.js"></script>
    <!-- End plugin js for this page -->
    <!-- inject:js -->
    <script src="~/assets/js/off-canvas.js"></script>
    <script src="~/assets/js/hoverable-collapse.js"></script>
    <script src="~/assets/js/misc.js"></script>
    <script src="~/assets/js/settings.js"></script>
    <script src="~/assets/js/todolist.js"></script>
    <script src="~/assets/js/jquery.cookie.js"></script>
    <!-- endinject -->
    <!-- Custom js for this page -->
    <script src="~/assets/js/dashboard.js"></script>
    <!-- End custom js for this page -->

  </body>
</html>
