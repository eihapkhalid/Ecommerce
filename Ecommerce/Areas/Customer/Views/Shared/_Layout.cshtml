

<!doctype html>
<html class="no-js" lang="ar">
<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>Uren - Car Accessories Shop HTML Template</title>
    <meta name="robots" content="noindex, follow" />
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Favicon -->
    <link rel="shortcut icon" type="image/x-icon" href="~/uren/assets/images/favicon.ico">

    <!-- CSS
    ============================================ -->
    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="~/uren/assets/css/vendor/bootstrap.min.css">
    <!-- Fontawesome -->
    <link rel="stylesheet" href="~/uren/assets/css/vendor/font-awesome.css">
    <!-- Fontawesome Star -->
    <link rel="stylesheet" href="~/uren/assets/css/vendor/fontawesome-stars.css">
    <!-- Ion Icon -->
    <link rel="stylesheet" href="~/uren/assets/css/vendor/ion-fonts.css">
    <!-- Slick CSS -->
    <link rel="stylesheet" href="~/uren/assets/css/plugins/slick.css">
    <!-- Animation -->
    <link rel="stylesheet" href="~/uren/assets/css/plugins/animate.css">
    <!-- jQuery Ui -->
    <link rel="stylesheet" href="~/uren/assets/css/plugins/jquery-ui.min.css">
    <!-- Lightgallery -->
    <link rel="stylesheet" href="~/uren/assets/css/plugins/lightgallery.min.css">
    <!-- Nice Select -->
    <link rel="stylesheet" href="~/uren/assets/css/plugins/nice-select.css">

    <!-- Vendor & Plugins CSS (Please remove the comment from below vendor.min.css & plugins.min.css for better website load performance and remove css files from the above) -->
    <!--
    <script src="~/uren/assets/js/vendor/vendor.min.js"></script>
    <script src="~/uren/assets/js/plugins/plugins.min.js"></script>
    -->
    <!-- Main Style CSS (Please use minify version for better website load performance) -->
    <link rel="stylesheet" href="~/uren/assets/css/style.css">
    <!--<link rel="stylesheet" href="~/uren/assets/css/style.min.css">-->

</head>

<style>
    /* تعديل اتجاه النص في العناصر */
    body {
        direction: rtl;
    }

    .nice-select::after {
        right: auto;
        left: 10px;
    }

    .select-search-category-wrapper {
        position: relative;
        display: inline-block;
    }

    .category-arrow {
        position: absolute;
        left: -10px;
        top: 50%;
        transform: translateY(-50%);
        color: #000;
        font-size: 18px;
    }
</style>

<body class="template-color-1 text-right">

    <div class="main-wrapper">

        <!-- Begin Uren's Header Main Area -->
        <header class="header-main_area header-main_area-2 bg--black">
            @await Component.InvokeAsync("MenuNavigation")
        </header>
        <!-- Uren's Header Main Area End Here -->
        <div class="container">
            <main role="main" class="pb-3">
                @RenderBody()
            </main>
        </div>

        <!-- Begin Uren's Footer Area -->
        <div class="uren-footer_area">
            <div class="footer-top_area">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-lg-12">

                            <!--Subscribe start-->
                            <div class="newsletter-area">
                                <h3 class="title">@Resources.JoinOurNewsletterNow</h3>
                                <p class="short-desc">@Resources.GetEmailupdatesaboutourlatestshopandspecialoffers</p>
                                <div class="newsletter-form_wrap">
                                    <form action="http://devitems.us11.list-manage.com/subscribe/post?u=6bbb9b6f5827bd842d9640c82&amp;id=05d85f18ef" method="post" id="mc-embedded-subscribe-form" name="mc-embedded-subscribe-form" class="newsletters-form validate" target="_blank" novalidate>
                                        <div id="mc_embed_signup_scroll">
                                            <div id="mc-form" class="mc-form subscribe-form">
                                                <input id="mc-email" class="newsletter-input" type="email" autocomplete="off" placeholder="@Resources.Enteryouremail" />
                                                <button class="newsletter-btn" id="mc-submit">@Resources.Subscribe</button>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>
                            <!--Subscribe end-->

                        </div>
                    </div>
                </div>
            </div>
            <div class="footer-middle_area">
                @await Component.InvokeAsync("FooterNavigation")
            </div>

            <!--Templateshub start--
            <div class="footer-bottom_area">
                <div class="container-fluid">
                    <div class="footer-bottom_nav">
                        <div class="row">
                            <div class="col-lg-6 col-md-6">
                                <div class="copyright">
                                    <span><a href="templateshub.net">Templateshub</a></span>
                                </div>
                            </div>
                            <div class="col-lg-6 col-md-6">
                                <div class="payment">
                                    <a href="#">
                                        <img src="~/uren/assets/images/footer/payment/1.png" alt="Uren's Payment Method">
                                    </a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <--Templateshub end1-->

        </div>
        <!-- Uren's Footer Area End Here -->
        <!-- JS
        ============================================ -->
        <!-- jQuery JS -->
        <script src="~/uren/assets/js/vendor/jquery-1.12.4.min.js"></script>
        <!-- Modernizer JS -->
        <script src="~/uren/assets/js/vendor/modernizr-2.8.3.min.js"></script>
        <!-- Popper JS -->
        <script src="~/uren/assets/js/vendor/popper.min.js"></script>
        <!-- Bootstrap JS -->
        <script src="~/uren/assets/js/vendor/bootstrap.min.js"></script>

        <!-- Slick Slider JS -->
        <script src="~/uren/assets/js/plugins/slick.min.js"></script>
        <!-- Barrating JS -->
        <script src="~/uren/assets/js/plugins/jquery.barrating.min.js"></script>
        <!-- Counterup JS -->
        <script src="~/uren/assets/js/plugins/jquery.counterup.js"></script>
        <!-- Nice Select JS -->
        <script src="~/uren/assets/js/plugins/jquery.nice-select.js"></script>
        <!-- Sticky Sidebar JS -->
        <script src="~/uren/assets/js/plugins/jquery.sticky-sidebar.js"></script>
        <!-- Jquery-ui JS -->
        <script src="~/uren/assets/js/plugins/jquery-ui.min.js"></script>
        <script src="~/uren/assets/js/plugins/jquery.ui.touch-punch.min.js"></script>
        <!-- Lightgallery JS -->
        <script src="~/uren/assets/js/plugins/lightgallery.min.js"></script>
        <!-- Scroll Top JS -->
        <script src="~/uren/assets/js/plugins/scroll-top.js"></script>
        <!-- Theia Sticky Sidebar JS -->
        <script src="~/uren/assets/js/plugins/theia-sticky-sidebar.min.js"></script>
        <!-- Waypoints JS -->
        <script src="~/uren/assets/js/plugins/waypoints.min.js"></script>
        <!-- jQuery Zoom JS -->
        <script src="~/uren/assets/js/plugins/jquery.zoom.min.js"></script>

        <!-- Vendor & Plugins JS (Please remove the comment from below vendor.min.js & plugins.min.js for better website load performance and remove js files from avobe) -->
        <!--
        <script src="~/uren/assets/js/vendor/vendor.min.js"></script>
        <script src="~/uren/assets/js/plugins/plugins.min.js"></script>
        -->
        <!-- Main JS -->
        <script src="~/uren/assets/js/main.js"></script>

        @await RenderSectionAsync("Scripts", required: false)
</body>
</html>